{
  "compilerOptions": {
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react-jsx",
    "module": "commonjs",
    "noEmit": true,
    "noUncheckedIndexedAccess": true,
    "paths": {"@/*": ["./lib/*"]},
    "skipLibCheck": true,
    "strict": true,
    "target": "es2016",
  },
  "include": ["./lib/**/*.ts", "./lib/**/*.tsx"],
  "exclude": ["node_modules"]
}
