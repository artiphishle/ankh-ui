    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/artiphishle/ankh-ui.git"
  },
  "dependencies": {
    "ahooks": "^3.8.0",
    "lucide-react": "^0.412.0",
    "next": "^14.2.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-use": "^17.5.1",
    "react-useportal": "^1.0.19"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.7",
    "@chromatic-com/storybook": "^1.6.1",
    "@storybook/addon-essentials": "^8.2.4",
    "@storybook/addon-interactions": "^8.2.4",
    "@storybook/addon-links": "^8.2.4",
    "@storybook/addon-onboarding": "^8.2.4",
    "@storybook/blocks": "^8.2.4",
    "@storybook/nextjs": "^8.2.4",
    "@storybook/react": "^8.2.4",
    "@storybook/test": "^8.2.4",
    "@types/node": "^20.14.11",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "eslint": "^9.7.0",
    "eslint-config-google": "^0.14.0",
    "prettier": "^3.3.3",
    "storybook": "^8.2.4",
    "tsup": "^8.1.2",
    "typescript": "^5.5.3"
  },
  "scripts": {
    "build": "tsup lib/index.ts --format cjs,esm --dts",
    "changeset:add": "pnpm changeset",
    "prettier:validate": "pnpm dlx prettier --config .prettierrc --write .",
    "prettier:write": "pnpm dlx prettier --config .prettierrc --write .",
    "lint:validate": "tsc && pnpm dlx eslint",
    "lint:fix": "tsc && pnpm eslint --fix",
    "prepublish": "pnpm run build",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  },
  "types": "dist/index.d.ts",
  "version": "1.4.1"
}
